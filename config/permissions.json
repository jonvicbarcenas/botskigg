{
  "roles": {
    "owner": {
      "users": ["YourMinecraftUsername"],
      "permissions": ["*"]
    },
    "admin": {
      "users": [],
      "permissions": [
        "bot.control",
        "bot.teleport",
        "bot.combat",
        "bot.inventory",
        "bot.status",
        "bot.restart"
      ]
    },
    "moderator": {
      "users": [],
      "permissions": [
        "bot.control",
        "bot.status",
        "bot.inventory"
      ]
    },
    "user": {
      "users": [],
      "permissions": [
        "bot.status"
      ]
    }
  },
  "commandPermissions": {
    "come": "bot.control",
    "follow": "bot.control",
    "stop": "bot.control",
    "goto": "bot.teleport",
    "attack": "bot.combat",
    "defend": "bot.combat",
    "inv": "bot.inventory",
    "give": "bot.inventory",
    "status": "bot.status",
    "restart": "bot.restart",
    "help": "bot.status"
  },
  "settings": {
    "defaultRole": "user",
    "allowPublicCommands": false,
    "logUnauthorizedAttempts": true
  }
}
